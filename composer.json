{
    "name": "designmynight/laravel-mongodb-passport",
    "description": "A package to allow laravel/passport use with jenssegers/laravel-mongodb",
    "homepage": "https://github.com/designmynight/laravel-mongodb-passport",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-mongodb",
        "laravel-mongodb-passport",
        "laravel-passport",
        "mongodb",
        "passport",
        "designmynight"
    ],
    "require": {
        "php": ">=7.4",
        "nyholm/psr7": "^1.0",
        "lcobucci/jwt": "^4.0",
        "illuminate/support": "^8.0",
        "jenssegers/mongodb": "^3.8 || ^4.0",
        "laravel/passport": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "DesignMyNight\\Mongodb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "DesignMyNight",
            "email": "support@designmynight.com",
            "role": "Support"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "DesignMyNight\\Mongodb\\MongodbPassportServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
