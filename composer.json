{
    "name": "sysvale/laravel-mongodb-passport",
    "description": "A package to allow laravel/passport use with jenssegers/laravel-mongodb",
    "homepage": "https://github.com/Sysvale/laravel-mongodb-passport",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel-mongodb",
        "laravel-mongodb-passport",
        "laravel-passport",
        "mongodb",
        "passport",
        "sysvale"
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "jenssegers/mongodb": "3.3.* || 3.4.* || 3.5.* || 3.6.* || 3.8.* || 3.9.*",
        "laravel/passport": "6.0.* || 7.0.* || 7.4.* || 7.5.* || ^8.0 || ^9.0 || ^10 || ^11"
    },
    "autoload": {
        "psr-4": {
            "Sysvale\\Mongodb\\": "src"
        }
    },
    "authors": [
        {
            "name": "DesignMyNight",
            "email": "support@designmynight.com"
        },
        {
            "name": "Geidson",
            "email": "geidson@sysvale.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Sysvale\\Mongodb\\MongodbPassportServiceProvider"
            ]
        }
    }
}
